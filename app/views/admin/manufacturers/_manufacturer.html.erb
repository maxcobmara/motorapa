<div id="<%= dom_id manufacturer %>">
  <p>
    <% if manufacturer.logo.attached? %>
      <%= image_tag manufacturer.logo, style: 'height:150px;width:auto;'  %>
    <% end %>
  </p>
    
  <p>
    <strong>Name:</strong>
    <%= manufacturer.name %>
  </p>

  <p>
    <strong>Website:</strong>
    <%= manufacturer.website %>
  </p>

  <p>
    <strong>Country:</strong>
    <%= manufacturer.country %>
  </p>

  <p>
    <strong>Data:</strong>
    <%= manufacturer.data %>
  </p>

</div>


